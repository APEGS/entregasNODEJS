<div>
    <h1>Bienvenido {{firstName}}</h1>
    
    <ul>
        <li>{{age}}</li>
        <li>{{email}}</li>
        <li>{{phone}}</li>
    </ul>

    {{#if isAdmin}}
        <ul>
        {{#each users}}
            <li>{{this.id}}, {{this.firstName}}, {{this.lastName}}</li>
        {{/each}}
        </ul>
    {{/if}}
</div>